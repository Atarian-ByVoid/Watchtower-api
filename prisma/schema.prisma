generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id    Int      @id @default(autoincrement())
    name  String
    email String   @unique
    uf String
    city String
    ibgecode    String
    law Boolean
    bidding Boolean
    news Boolean
    covid Boolean
    dengue Boolean
}

enum News {
  LAW
  BIDDING
  NEWS
  COVID
  DENGUE
}

