
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String
  news     News   @default(LAW)


  @@map("users")
}

enum News {
  LAW
  BIDDING
  NEWS
  COVID
  DENGUE
}